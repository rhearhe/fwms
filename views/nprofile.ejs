<%- include("partials/header") -%>
<div class="nprofile" style="padding-bottom:20rem">
  <div class="head container-fluid">
    <nav class="hpnavbar navbar navbar-expand-lg navbar-dark bg-dark">
      <a href="#" class="navbar-brand feedly">Feedly</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="/">Home</a></li>
          <li class="nav-item"><a class="nav-link" href="/ngo">Main Page</a></li>
        </ul>
      </div>
    </nav>
  </div><br><br><br><br><br><br><br>
<div class="container" style="text-align:center;">
<form action="/nprofile" method="POST">
  <input type="text" name="search" style="width:30%;padding:6px;" placeholder="Re-enter Unique ID">
  <button class="btn btn-md btn-primary" type="submit">submit</button>
</form>
</div><br><br><br><br><br><br>
<div class="container" id="details">
  <form>
  <%doc.forEach(function(found){%>
    <h1>Details</h1><h5>
    NGO name: <%=found.name%><br>
    NGO Type: <%=found.type%><br>
    NGO Unique ID: <%=found.uniqueid%><br>
    Phone number: <%=found.phno%><br>
    Email: <%=found.email%><br>
    Address: <%=found.address%><br>
    Documents: <%=found.documents%>
  <%})%></h5>
  </form>
</div>
</div>
<%- include("partials/footer") -%>
