<%- include("partials/header") -%>
<div class="rprofile"  style="
background-image: url(../Images/food.jpg);
background-attachment: fixed;
background-color: #D8E9A8;
background-size: cover;
padding-bottom: 18rem;
text-align: center">
<div class="head container-fluid" style="padding-left:0; padding-right:0;">
  <nav class="hpnavbar navbar navbar-expand-lg navbar-dark bg-dark" >
    <a href="#" class="navbar-brand feedly">Feedly</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link" href="/">Home</a></li>
          <%doc.forEach(function(found){%>
        <li class="nav-item"><a class="nav-link" href='/restaurant/<%=found.fssai%>'>Main Page</a></li>
        <%})%>
      </ul>
    </div>
  </nav>
</div><br><br>
<div class="container restDeets" style=" width:30%;font-size:20px;border: 2px solid black;text-align:center">
  <!-- position:relative;left:30rem;top:5rem;-->
  <%doc.forEach(function(found){%>
  <h1>Details</h1>
  Restaurant name: <%=found.name%><br>
  Restaurant FSSAI: <%=found.fssai%><br>
  Phone number: <%=found.phno%><br>
  Email: <%=found.email%><br>
  Address: <%=found.address%><br>
  Documents: <%=found.documents%><br>
  <%})%>
</div>
</div>
<%- include("partials/footer") -%>
